{
  "name": "portofolio",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seeds.ts"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build && npm run postbuild",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --verbose",
    "postbuild": "next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "lighthouse": "lighthouse https://ansyar-world.top --output html --output-path ./lighthouse-report.html",
    "seo:check": "npm run lighthouse && echo 'SEO report generated'",
    "validate:html": "html-validate public/**/*.html || echo 'HTML validation complete'"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@tabler/icons-react": "^3.34.0",
    "@types/three": "^0.177.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.16.0",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^7.0.3",
    "prisma": "^6.8.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.2",
    "simplex-noise": "^4.0.3",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.0",
    "three": "^0.177.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@jest/types": "30.0.0-beta.3",
    "@tailwindcss/postcss": "^4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.57",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "30.0.0-beta.3",
    "msw": "^2.9.0",
    "prisma-mock": "^0.10.3",
    "tailwindcss": "^4.1.8",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  }
}
